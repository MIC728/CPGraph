### This is sample file of .env

### Logging level
LOG_LEVEL=INFO
# VERBOSE=False
# LOG_MAX_BYTES=10485760
# LOG_BACKUP_COUNT=5
### Logfile location (defaults to current working directory)
# LOG_DIR=/path/to/log/directory


########################################
### Document processing configuration
########################################
ENABLE_LLM_CACHE_FOR_EXTRACT=true

### Document processing output language: English, Chinese, French, German ...
SUMMARY_LANGUAGE=Chinese

### Entity types that the LLM will attempt to recognize
### Two-dimensional entity types for enhanced classification
# 第一维度（技术分类）
ENTITY_TYPES_DIM1='["数据结构", "算法思想", "动态规划", "图论", "搜索", "字符串", "数学", "计算几何"]'
# 第二维度（应用层次）
ENTITY_TYPES_DIM2='["概念", "技巧", "实现", "模型", "算法", "原理", "题目"]'

### Chunk size for document splitting, 500~1500 is recommended
CHUNK_SIZE=1200
CHUNK_OVERLAP_SIZE=100

###############################
### Concurrency Configuration
###############################
### Max concurrency requests of LLM (for both query and document processing)
MAX_ASYNC=24
### Number of parallel processing documents(between 2~10, MAX_ASYNC/3 is recommended)
MAX_PARALLEL_INSERT=8
### Max concurrency requests for Embedding
EMBEDDING_FUNC_MAX_ASYNC=8
### Num of chunks send to Embedding in single request
EMBEDDING_BATCH_NUM=10

###########################################################################
### LLM Configuration (OpenAI Compatible Only)
###########################################################################
LLM_TIMEOUT=600

LLM_BINDING=openai
LLM_MODEL=deepseek-chat
LLM_BINDING_HOST=https://api.deepseek.com/v1
LLM_BINDING_API_KEY=sk-98885ca6a9a0409bb3bb9fa34d025321

### OpenAI Compatible API Parameters
OPENAI_LLM_MAX_COMPLETION_TOKENS=9000

#######################################################################################
### Embedding Configuration (OpenAI Compatible Only)
#######################################################################################
EMBEDDING_BINDING=openai
EMBEDDING_MODEL=Qwen/Qwen3-Embedding-4B
EMBEDDING_DIM=2560
EMBEDDING_BINDING_API_KEY=sk-qfxthqixorsxyaviencdqclbbolkzfwmcgikkzkjsyxyjgtd
EMBEDDING_BINDING_HOST=https://api.siliconflow.cn/v1


### Neo4j Configuration
#NEO4J_URI=neo4j+s://xxxxxxxx.databases.neo4j.io
NEO4J_URI=bolt://127.0.0.1:7687
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=00000000
NEO4J_DATABASE=neo4j
NEO4J_MAX_CONNECTION_POOL_SIZE=100
NEO4J_CONNECTION_TIMEOUT=30
NEO4J_CONNECTION_ACQUISITION_TIMEOUT=30
NEO4J_MAX_TRANSACTION_RETRY_TIME=30
NEO4J_MAX_CONNECTION_LIFETIME=300
NEO4J_LIVENESS_CHECK_TIMEOUT=30
NEO4J_KEEP_ALIVE=true
# NEO4J_WORKSPACE=forced_workspace_name

###########################
### Performance Profiling
###########################
### Enable timing for performance analysis
LIGHTRAG_ENABLE_TIMING=true
LIGHTRAG_TIMING_MIN_DURATION=0.1
LIGHTRAG_TIMING_MAX_DEPTH=10

########################################
### Relation Schema Configuration
########################################
### Enable relation schema for standardized relationship types
### Supported relation types: IS_A, PART_OF, BASED_ON, APPLIES_TO, ANALYZES, COMPARES_WITH, LEADS_TO, OPTIMIZES, TRANSFORMS_TO
# RELATION_SCHEMA_ENABLED=true
### Separator for multiple relation tags (comma-separated)
### When a relationship has multiple types, separate them with this separator
# RELATION_SCHEMA_TAG_SEP=","
EMBEDDING_MAX_CONCURRENT=16

#######################################################################################
### LLM Usage Tracking Configuration
### Enable LLM usage tracking for monitoring API calls, token usage, and costs
#######################################################################################
ENABLE_LLM_TRACKING=true

#######################################################################################
### Entity Merge Configuration (entity_merge.py)
### 实体合并模块配置
#######################################################################################
### 增量更新开关：true=增量更新（读取已有数据后合并），false=全量覆盖
ENTITY_MERGE_INCREMENTAL=false
### merged_data 目录路径（存放合并后的实体、关系、chunks备份）
MERGED_DATA_DIR=./merged_data


###########################
### File Extractor Configuration
###########################
EXTRACTOR_OUTPUT_DIR=./extracted_data


###########################
### Entity Merge Configuration
###########################
ENTITY_MERGE_INPUT_DIR=./extracted_data
ENTITY_MERGE_OUTPUT_DIR=./merged_data